@page
@model IndexModel
@{
    ViewData["Title"] = "Home page";
}

@if(Model.Users != null && Model.Users.Count > 0)
{
    @foreach(var user in Model.Users)
    {
        <div>
            <h1>@user.FirstName @user.LastName</h1>
            <img src="@user.Image" style="max-width:200px;"/>
        </div>
    }
}